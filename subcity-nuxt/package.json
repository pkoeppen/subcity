{
  "name": "subcity-nuxt",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Peter Koeppen <peter@sub.city>",
  "private": true,
  "scripts": {
    "deploy": "NODE_ENV=prod nuxt build && serverless deploy --stage prod --domain sub.city",
    "devdeploy": "nuxt build && serverless deploy --stage dev --domain dev.sub.city",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cookieparser": "^0.1.0",
    "express": "^4.16.4",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "nuxt": "^1.0.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-http": "^1.9.0",
    "vue": "2.5.21",
    "vue-material": "latest",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "serverless-domain-manager": "^2.6.12",
    "serverless-offline": "^3.33.0"
  }
}
